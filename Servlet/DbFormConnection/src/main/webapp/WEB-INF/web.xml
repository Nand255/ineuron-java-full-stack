<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  
  <servlet>
    <servlet-name>TestApp</servlet-name>
    <servlet-class>in.ineuron.controller.TestApp</servlet-class>
    
    <init-param>
      <param-name>url</param-name>
      <param-value>jdbc:mysql://localhost:3306/student</param-value>
    </init-param>
    <init-param>
      <param-name>username</param-name>
      <param-value>root</param-value>
    </init-param>
    <init-param>
      <param-name>password</param-name>
      <param-value>2525</param-value>
    </init-param>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>TestApp</servlet-name>
    <url-pattern>/TestApp</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>RetriveInfo</servlet-name>
    <servlet-class>in.ineuron.controller.RetriveInfo</servlet-class>
    
    <init-param>
      <param-name>url</param-name>
      <param-value>jdbc:mysql://localhost:3306/student</param-value>
    </init-param>
    <init-param>
      <param-name>username</param-name>
      <param-value>root</param-value>
    </init-param>
    <init-param>
      <param-name>password</param-name>
      <param-value>2525</param-value>
    </init-param>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>RetriveInfo</servlet-name>
    <url-pattern>/retrive</url-pattern>
  </servlet-mapping>
  
</web-app>